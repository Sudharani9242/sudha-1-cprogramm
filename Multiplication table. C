
#include <stdio.h>

int main() {
int num;
printf("enter a number:");
scanf("%d", &num) ;
for(int I  =1;I<=10;I++){
    printf("%dx %d = %d\n", num, I, num *I) ;
}
return 0;
}enter a number:2 

2x 1 = 2

2x 2 = 4

2x 3 = 6

2x 4 = 8

2x 5 = 10

2x 6 = 12

